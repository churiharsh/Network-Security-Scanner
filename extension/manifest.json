{
    "manifest_version": 2,
  
    "name": "Website blocker",
    "description": "This extension blocks unsecure Websites",
    "version": "1.0",
    "browser_action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "webRequest",
      "*://*.google.com/",
      "activeTab",
      "https://ajax.googleapis.com/"
    ],
    "background": {
      "scripts": ["./funcs.js"],
      "persistent": true
    }
  }